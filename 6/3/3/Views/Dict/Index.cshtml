
@model List<PhoneRepositorySql.Phone>
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Phone Book</title>
</head>
<body>
    @section head {
        <h2>Phone Book by Pochta</h2>
    }
    <div>
        <h3>All phones info</h3> <a href="/Dict/Add/">Create new phone</a>
        <div>
            <table>
                <tr>
                    <td><p>Name</p></td>
                    <td><p>Number</p></td>
                    <td></td>
                </tr>
                @foreach (var b in Model)
                {
                    @Html.Partial("PartOfView", (object)b)
                }
            </table>
        </div>
    </div>
</body>
</html>
