@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Phone Book</title>
</head>
<body>
    <div>
        <h3>All phones info</h3> <a href="/Dict/Add/">Create new phone</a>
        <div><table>
            <tr>
                <td><p>Id</p></td>
                <td><p>Name</p></td>
                <td><p>Number</p></td>
                <td></td>
            </tr>
            @foreach (var b in ViewBag.Phones)
            {
                <tr>
                    <td><p>@b.Id</p></td>
                    <td><p>@b.Name</p></td>
                    <td><p>@b.Number</p></td>
                    <td><p><a href="/Dict/DeleteSave/@b.Id">Delete</a></p></td>
                    <td><p><a href="/Dict/Update/@b.Id">Update</a></p></td>
                </tr>
            }
        </table></div>
    </div>
</body>
</html>